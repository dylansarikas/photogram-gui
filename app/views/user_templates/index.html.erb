<h1> List of Users </h1>

<p>add user</p>

<table border="1">
  <tr>
    <th>ID</th>

    <th>Username</th>

    <th></th>
  </tr>

  <% @all_users.each do |user| %>

    <tr>
      <td>
        <%= user.id %>
      </td>

      <td>
        <%= user.username  %>
      </td>

      <td>
        <a href="/users/<%= user.username  %>">Show details</a>
      </td>
    </tr>


  <% end%>
  

</table>
